{
  "name": "tree-house-errors",
  "version": "1.2.0",
  "description": "NodeJS default error definitions with an error parser utility function",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/cli.js --no-cleanup"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "express-validation": "~1.0.2",
    "http-status": "~1.2.0",
    "i18n": "~0.8.3",
    "joi": "~14.0.4",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "@types/http-status": "~0.2.30",
    "@types/i18n": "~0.8.3",
    "@types/jest": "~23.3.9",
    "@types/uuid": "~3.4.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "np": "^3.0.4",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "ts-jest": "~22.4.6",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "tree house",
    "treehouse",
    "errors"
  ],
  "license": "ISC",
  "repository": "git@github.com:icapps/tree-house-errors.git",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-errors/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-errors#readme"
}
